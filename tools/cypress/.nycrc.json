{
  "cache": false,
  "extension": [
    ".ts",
    ".tsx"
  ],
  "exclude": [
    "**/*.d.ts",
    "coverage/**",
    "packages/*/test/**",
    "test/**",
    "test{,-*}.ts",
    "**/*{.,-}{test,spec}.ts",
    "**/__tests__/**",
    "**/node_modules/**"
  ],
  "all": true,
  "check-coverage": true,
  "require": [
    "ts-node/register"
  ],
  "temp-directory": ".nyc_output",
  "sourceMap": false,
  "instrument": false,
  "include": ["src/**/*.ts", "src/**/*.tsx"],
  "exclude-after-remap": false
}