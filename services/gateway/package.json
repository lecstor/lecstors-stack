{
  "name": "gateway",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "gateway:build": "pnpify tsc -p .",
    "tsc": "pnpify tsc",
    "gateway:dev": "yarn run ts-node-dev --transpileOnly --no-notify ./src/server.ts",
    "gateway:local": "NODE_CONFIG_ENV=localhost NODE_ENV=development yarn run ts-node-dev --transpileOnly --no-notify ./src/server.ts",
    "gateway:start": "yarn node ./dist/src/server.js",
    "gateway:start:local": "NODE_CONFIG_ENV=localhost yarn node ./dist/src/server.js",
    "make-migration": "pnpify knex migrate:make -x ts",
    "db:migrate": "pnpify knex migrate:latest",
    "db:migrate-rollback": "pnpify knex migrate:rollback",
    "db:make-seed": "pnpify knex seed:make",
    "db:seed": "pnpify knex seed:run",
    "gateway:test": "NODE_CONFIG_ENV=${NODE_CONFIG_ENV:-localhost} echo ${NODE_CONFIG_ENV:-blah}"
  },
  "dependencies": {
    "@types/cors": "^2.8.6",
    "amqplib": "^0.5.5",
    "apollo-server-express": "^2.8.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "config": "^3.2.2",
    "connect-pg-simple": "^6.0.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "graphql": "^14.5.4",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.5",
    "knex": "^0.19.2",
    "morgan": "^1.9.1",
    "objection": "^1.6.9",
    "passport": "^0.4.0",
    "passport-strategy": "^1.0.0",
    "pg": "^7.12.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.13",
    "@types/compression": "^1.0.1",
    "@types/config": "0.0.34",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.14",
    "@types/graphql": "14.5.0",
    "@types/node": "^12.7.4",
    "@types/passport": "^1.0.1",
    "@types/passport-strategy": "0.2.35",
    "@types/uuid": "3.4.5",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.0.0",
    "@yarnpkg/pnpify": "^2.0.0-rc.3",
    "eslint": "^6.2.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "graphql-import-node": "^0.0.4",
    "ncp": "^2.0.0",
    "prettier": "^1.18.2",
    "ts-node": "^8.3.0",
    "ts-node-dev": "^1.0.0-pre.42",
    "typescript": "^3.6.2"
  }
}
