{
  "name": "@lecstor/gateway",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc -p .",
    "db:make-seed": "knex seed:make",
    "db:migrate-rollback": "knex migrate:rollback",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "db:make-migration": "knex migrate:make -x ts",
    "start:docker:dev": "yarn run ts-node-dev --transpileOnly --no-notify ./src/server.ts",
    "start:docker:dist": "yarn node ./dist/src/server.js",
    "start:local:dev": "COPIN_CONFIG_BUILD=local NODE_ENV=development yarn run ts-node-dev --transpileOnly --no-notify ./src/server.ts",
    "start:local:dist": "COPIN_CONFIG_BUILD=local yarn node ./dist/src/server.js",
    "start": "yarn start:local:dev",
    "tsc": "tsc"
  },
  "dependencies": {
    "@lecstor/config": "1.0.0",
    "@sendgrid/mail": "^6.5.2",
    "@types/cors": "^2.8.6",
    "amqplib": "^0.5.5",
    "apollo-server-express": "^2.9.12",
    "bcrypt": "^3.0.7",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-pg-simple": "^6.0.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "graphql": "^14.5.4",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.5",
    "knex": "^0.20.2",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "objection": "^2.0.10",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-strategy": "^1.0.0",
    "pg": "^7.14.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.13",
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.0.1",
    "@types/config": "0.0.36",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.16",
    "@types/node": "^12.12.11",
    "@types/passport": "^1.0.2",
    "@types/passport-strategy": "0.2.35",
    "@types/uuid": "3.4.6",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "eslint": "^6.2.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "graphql-import-node": "^0.0.4",
    "ncp": "^2.0.0",
    "prettier": "^1.18.2",
    "ts-node": "^8.3.0",
    "ts-node-dev": "^1.0.0-pre.42",
    "typescript": "^3.6.2"
  }
}
