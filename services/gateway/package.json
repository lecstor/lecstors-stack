    "@types/amqplib": "^0.5.13",
    "@types/async-retry": "^1.4.2",
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.0.1",
    "@types/config": "0.0.36",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.16",
    "@types/graphql-resolvers": "^0.3.0",
    "@types/jest": "^26.0.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.13",
    "@types/passport": "^1.0.2",
    "@types/passport-local": "^1.0.33",
    "@types/passport-strategy": "0.2.35",
    "@types/uuid": "8.0.0",
    "amqp-connection-manager": "^3.2.0",
    "amqplib": "^0.5.5",
    "apollo-server-express": "^2.14.1",
    "apollo-server-testing": "^2.14.4",
    "async-retry": "^1.3.1",
    "bcrypt": "^5.0.0",
    "bitset": "^5.1.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-pg-simple": "^6.0.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cuid": "^2.1.8",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "graphql": "^15.1.0",
    "graphql-parse-resolve-info": "^4.7.0",
    "graphql-resolvers": "^0.4.2",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^6.0.9",
    "jest": "^26.0.1",
    "knex": "^0.21.1",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "objection": "2.2.0",
    "objection-graphql": "^0.4.5",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-strategy": "^1.0.0",
    "pg": "^8.2.1",
    "ts-node": "^8.3.0",
    "ts-node-dev": "^1.0.0-pre.42",
    "typescript": "^3.6.2",
